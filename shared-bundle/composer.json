{
    "name": "microservices/shared-bundle",
    "description": "Shared bundle for Product and Order microservices",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.3",
        "symfony/uid": "^7.3",
        "doctrine/doctrine-bundle": "^2.12",
        "symfony/messenger": "^7.3",
        "doctrine/orm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Microservices\\SharedBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microservices\\SharedBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.48",
        "rector/rector": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

